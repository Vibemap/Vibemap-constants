{
  "name": "vibemap-constants",
  "version": "1.2.6",
  "description": "Design system and tokens. Other shared constants and configurations.",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run fetch-taxonomies",
    "build-design-system": "npm run build-semantic && cd design-system && style-dictionary build",
    "build-semantic": "cd design-system/semantic && npx gulp build-css build-assets",
    "watch-semantic": "cd design-system/semantic && yarn run build-semantic && gulp watch",
    "get-figma-styles": "cd design-system && node sync-figma.js",
    "build-all-constants": "npm run build-constants && npm run build-dictionary && npm run build-badges && npm run build-routes && npm run build-categories && npm run build-vibes",
    "build-categories": "cd constants && js-yaml categories.yml > '../dist/categories.json'",
    "build-constants": "cd constants && js-yaml constants.yml > constants.json",
    "build-dictionary": "cd design-system && style-dictionary build",
    "build-routes": "cd routes && js-yaml routes.yml > routes.json",
    "build-vibes": "cd constants && js-yaml vibes.yml > '../dist/vibes.json'",
    "build-helpers": "rollup -c",
    "fetch-taxonomies": "node scripts/fetch-taxonomies",
    "test": "echo \"No tests for this repository\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "design-system",
    "design-system/assets/logos",
    "dist",
    "routes",
    "translations",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevepepple/Vibemap-constants.git"
  },
  "keywords": [
    "design",
    "system",
    "routes",
    "constants"
  ],
  "author": "Vibemap, Inc.",
  "bugs": {
    "url": "https://github.com/stevepepple/Vibemap-constants/issues"
  },
  "homepage": "https://github.com/stevepepple/Vibemap-constants#readme",
  "dependencies": {
    "@mapbox/geo-viewport": "^0.4.1",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/preset-scss": "^1.0.3",
    "@teambit/bvm": "^0.0.20",
    "@turf/turf": "^5.1.6",
    "axios": "^0.21.1",
    "bit-bin": "^14.8.8",
    "browser-monads": "^1.0.0",
    "chroma-js": "^2.1.0",
    "cross-env": "^7.0.3",
    "d3-array": "^2.11.0",
    "d3-scale": "^3.2.3",
    "dayjs": "^1.9.6",
    "figgo": "^0.2.4",
    "fuse.js": "^6.4.3",
    "global": "^4.4.0",
    "gulp-clone": "^2.0.1",
    "internmap": "^1.0.0",
    "js-yaml": "^3.14.0",
    "json2yaml": "^1.1.0",
    "less-loader": "^8.1.1",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "mini-css-extract-plugin": "^1.5.0",
    "production": "0.0.2",
    "react-inlinesvg": "^2.2.2",
    "request": "^2.88.2",
    "sass-loader": "^10.1.1",
    "style-dictionary": "^2.10.0",
    "truncate": "^2.1.0",
    "url": "^0.11.0",
    "write-json": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@rollup/plugin-json": "^4.1.0",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "fomantic-ui": "^2.8.6",
    "gulp": "^4.0.2",
    "rollup": "^2.34.0",
    "sass": "^1.32.11",
    "style-loader": "^2.0.0"
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
